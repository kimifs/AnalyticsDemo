    <validationRules>
        <fullName>Valid_Max_No_Of_Flats</fullName>
        <active>true</active>
        <errorConditionFormula>Building__r.Number_Of_Flats__c  &gt;=  Building__r.Maximum_Number_of_Flats__c</errorConditionFormula>
