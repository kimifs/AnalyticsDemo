        <fullName>Balcony_should_not_be_on_floor_1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Has_Balcony__c , Floor__c = 1 )</errorConditionFormula>
        <errorDisplayField>Has_Balcony__c</errorDisplayField>
